# Gnuplot script file for plotting data in file "force.dat"
# This file is called   force.p
set autoscale
set datafile separator ","
unset log
set key left
#unset label
#unset title
#unset key
f(x) = a*x
fit f(x) "../results/gpu1.out" using 2:4 via a
g(x) = b*x
fit g(x) "../results/gpu_global1.out" using 2:5 via b
set xtic auto
set ytic auto 
set title "GPU Implementation Comparison"
set ylabel "Labeling Time (ms)"
set xlabel "No. pixels"
#set size 1.5, 1.5
#set xr [0.0:0.022]
#set yr [0:325]
#set xtics 500000,500000,10000000
#set terminal epslatex oldstyle
#set output "allInteg.eps"
set terminal png size 1280,720 enhanced
set output "gp_gpu_vs.png"
plot "../results/gpu1.out"  using 2:4 w points t "GPU Unionfind (standard)", \
     "../results/gpu_global1.out" using 2:5 w points t "GPU Unionfind (global memory)", \
     f(x) t "f(x) = a*x", \
     g(x) t "g(x) = b*x", \